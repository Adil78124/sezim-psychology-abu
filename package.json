{
  "name": "sezim-psychology-react",
  "version": "1.0.0",
  "private": true,
  "description": "Центр психологической поддержки Sezim.abu - React приложение",
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "dev:frontend": "react-scripts start",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build:backend": "cd backend && npm install",
    "build:all": "npm run build:backend && npm run build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "vercel-build": "npm run build",
    "install:all": "npm install && cd backend && npm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://anubizze.github.io/psychology",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}