{
  "name": "sezim-psychology-react",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Центр психологической поддержки Sezim.abu - React приложение",
  "scripts": {
    "dev": "vite",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "vite build",
    "build:backend": "cd backend && npm install",
    "build:all": "npm run build:backend && npm run build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "vercel-build": "rm -rf node_modules package-lock.json && npm install && vite build",
    "start": "npm run dev:all",
    "install:all": "npm install && cd backend && npm install"
  },
  "homepage": "https://anubizze.github.io/psychology",
  "dependencies": {
    "@supabase/supabase-js": "^2.76.1",
    "firebase": "^12.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "vite": "^7.1.7"
  }
}
